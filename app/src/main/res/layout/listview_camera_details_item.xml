        android:listSelector="#00ffffff"
        android:numColumns="3"
        android:verticalSpacing="10dp"
        android:layout_below="@id/head_relative_layout">
    </com.isoftstone.smartsite.model.tripartite.view.MyGridView>

</RelativeLayout>